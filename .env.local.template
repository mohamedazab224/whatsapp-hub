# تحذير: هذا ملف للتطوير فقط - لا تستخدم هذه القيم في الإنتاج
# Supabase (client)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase (server)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Auth/session (legacy/basic auth)
BASIC_AUTH_USERS=admin@example.com:secure_password:admin
AUTH_PASSWORD_SALT=your_secure_salt_here
SESSION_SECRET=your_secure_session_secret_here

# WhatsApp webhook + API
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your_webhook_verify_token
WHATSAPP_APP_SECRET=your_app_secret
WHATSAPP_ACCESS_TOKEN=your_access_token
WHATSAPP_API_VERSION=v21.0

# Queue worker
QUEUE_SECRET=your_queue_secret
QUEUE_RATE_LIMIT_MAX=30
QUEUE_RATE_LIMIT_WINDOW_SEC=60

# Webhook rate limiting
WEBHOOK_RATE_LIMIT_MAX=120
WEBHOOK_RATE_LIMIT_WINDOW_SEC=60

# Logging
LOG_LEVEL=info